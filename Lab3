#include<math.h>
#include<stdlib.h>
#include<stdio.h>

using namespace std;

int main()
{
	double Mult = 1;
	int minplu=1;
	int n;
	printf_s("n=");
	scanf_s("%d", &n);
	for(int k = 1; k<n; k++)
	{
		double modl = (n - k) / pow(k, k);
		Mult*= minplu*modl;
		minplu *= -1;
	}
	printf_s("Mult=%g", Mult);
	system("pause");
}
